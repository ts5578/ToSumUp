{
    "manifest_version": 3,
    "name": "ToSumUp",
    "version": "1.0",
    "description": "Summarizes emails in Gmail.",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://mail.google.com/",
      "http://127.0.0.1:5000/summarize"
    ]
  }
  
 